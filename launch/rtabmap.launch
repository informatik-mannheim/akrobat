<!-- -*- mode: XML -*- -->
<launch>
<!-- Visualisation RTAB-Map -->
  <node pkg="rtabmap_ros" type="rtabmapviz" name="rtabmapviz" ns="rtabmap" output="screen">
     
     <param name="frame_id"            type="string" value="left"/>
     <remap from="left/image_rect"   to="/akrobat_stereo/left/image_rect_color"/>
     <remap from="right/image_rect"  to="/akrobat_stereo/right/image_rect"/>
     <remap from="left/camera_info"  to="/akrobat_stereo/left/camera_info"/>
     <remap from="right/camera_info" to="/akrobat_stereo/right/camera_info"/>
     <remap from="odom_info"         to="/akrobat_stereo/odom_info"/>
     <remap from="odom"              to="/stereo_odometry"/>
  </node>
 </launch>
