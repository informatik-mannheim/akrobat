<launch>

    <!-- Run the map server --> 
    <!-- <node name="map_server" pkg="map_server" type="map_server" args="$(find akrobat)/map/carto-neu.pgm 0.050000"/> -->

    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find akrobat)/config/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
        <rosparam file="$(find akrobat)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find akrobat)/config/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find akrobat)/config/global_costmap_params.yaml" command="load" /> 
        <rosparam file="$(find akrobat)/config/base_local_planner_params.yaml" command="load" />
    </node>
</launch>